{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["."],
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
